{
  "name": "monitor-app",
  "version": "1.0.0",
  "description": "monitor-app",
  "scripts": {
    "test": "./node_modules/.bin/jsxhint lib/ spec/ && ./node_modules/.bin/jshint lib/ spec/ && ./node_modules/karma/bin/karma start karma.conf.js",
    "watch-test": "./node_modules/karma/bin/karma start karma.conf.js --auto-watch --no-single-run",
    "build": "./node_modules/.bin/webpack -p",
    "rename-css-bower-dependencies": "find bower_components -name '*.css' | sed -e 'p;s/.css$/.scss/' | xargs -n2 mv",
    "dev": "./node_modules/.bin/webpack-dev-server --inline --hot",
    "postinstall": "bower install"
  },
  "author": "",
  "license": "BSD",
  "devDependencies": {
    "babel-core": "^4.7.8",
    "babel-loader": "^4.1.0",
    "css-loader": "^0.9.1",
    "es5-shim": "^4.1.0",
    "jasmine": "^2.2.1",
    "jasmine-core": "^2.2.0",
    "jshint": "^2.6.0",
    "jsx-loader": "^0.12.2",
    "jsxhint": "^0.10.0",
    "karma": "^0.12.31",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-webpack": "^1.5.0",
    "react-hot-loader": "^1.2.3",
    "sass-loader": "^0.4.0-beta",
    "style-loader": "^0.8.3",
    "webpack": "^1.5.3",
    "webpack-dev-server": "^1.7.0"
  },
  "dependencies": {
    "lodash": "^3.3.0",
    "promise": "^6.1.0",
    "react": "^0.12.2",
    "react-router": "^0.12.4",
    "superagent": "^0.21.0",
    "superagent-promise": "^0.2.0"
  }
}
